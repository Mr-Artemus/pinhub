{
    "name": "GitHub homepage pinned repositories",
    "version": "0.1.0",
    "manifest_version": 2,
    "description": "Adding a section that shows the pinned repositories on GitHub's homepage",
    "homepage_url": "https://github.com/Mr-Artemus/github-homepage-pinned-repos",
	"permissions": [
		"storage"
	],
    "content_scripts": [
		{
			"matches": [
				"*://github.com/*"
			],
			"js": [
				"src/main.js"
			]
		}
    ]
}