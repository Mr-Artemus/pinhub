{
    "name": "PinHub",
    "version": "0.1.0",
    "manifest_version": 2,
    "description": "A simple extension that allow you to pin your darone on the Github's homepage",
    "homepage_url": "https://github.com/Mr-Artemus/github-homepage-pinned-repos",
	"icons": {
	    "16": "src/icons/16.png",
	    "32": "src/icons/32.png",
	    "48": "src/icons/48.png",
	    "128": "src/icons/128.png"
	},
	"permissions": [
		"storage"
	],
    "content_scripts": [
		{
			"matches": [
				"*://github.com/*"
			],
			"js": [
				"src/main.js"
			]
		}
    ]
}